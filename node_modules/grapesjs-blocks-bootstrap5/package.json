{
  "name": "grapesjs-blocks-bootstrap5",
  "version": "0.2.31",
  "description": "GrapesJS Bootstrap v5 Blocks Plugin. Based on https://github.com/Eximiaweb/grapesjs-blocks-bootstrap5",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "author": {
    "name": "Adrian Schimpf",
    "email": "https://aivie.ch"
  },
  "scripts": {
    "deploy": "npm run build && npm run create-types && npm publish && git add . && git commit && git push",
    "lint": "eslint src",
    "v:patch": "npm version --no-git-tag-version patch",
    "v:minor": "npm version --no-git-tag-version minor",
    "build-dev": "npm run && webpack --env production",
    "create-types": "npx -p typescript tsc ./src/index.js --declaration --allowJs --emitDeclarationOnly --esModuleInterop --outDir ./dist",
    "start": "grapesjs-cli serve",
    "build": "grapesjs-cli build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aiviemarketing/grapesjs-blocks-bootstrap5.git"
  },
  "keywords": [
    "grapesjs",
    "plugin",
    "bootstrap"
  ],
  "devDependencies": {
    "grapesjs": "^1.0.0",
    "grapesjs-cli": "^4.1.3"
  }
}
